{
    "name": "frisklog-nest-server",
    "version": "0.0.1",
    "description": "frisklog backend repo",
    "author": "frisk",
    "private": true,
    "license": "MIT",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "cross-env NODE_ENV=production nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "cross-env NODE_ENV=production TZ=Asia/Seoul nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "cross-env NODE_ENV=production TZ=Asia/Seoul node dist/main",
        "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "echo 'No e2e tests implemented yet.'",
        "seed:platform": "ts-node src/database/seeds/platform.seeder.ts",
        "seed:platform:prod": "cross-env NODE_ENV=production ts-node src/database/seeds/platform.seeder.ts"
    },
    "dependencies": {
        "@graphql-tools/utils": "8.8.0",
        "@nestjs/apollo": "10.0.17",
        "@nestjs/axios": "^0.1.0",
        "@nestjs/common": "9.0.1",
        "@nestjs/config": "^2.2.0",
        "@nestjs/core": "9.0.1",
        "@nestjs/graphql": "10.0.18",
        "@nestjs/platform-express": "9.0.1",
        "@nestjs/typeorm": "9.0.0",
        "apollo-server": "3.9.0",
        "apollo-server-core": "^3.10.1",
        "apollo-server-express": "3.9.0",
        "apollo-server-plugin-base": "^3.6.2",
        "axios": "^0.27.2",
        "class-transformer": "0.5.1",
        "class-validator": "0.13.2",
        "dotenv": "^16.0.1",
        "graphql": "16.5.0",
        "graphql-subscriptions": "2.0.0",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.29.4",
        "mysql2": "2.3.3",
        "nodemailer": "^6.7.8",
        "nodemailer-smtp-pool": "^2.8.3",
        "reflect-metadata": "0.1.13",
        "rimraf": "3.0.2",
        "rxjs": "7.5.5",
        "typeorm": "0.3.7",
        "typeorm-cursor-pagination": "^0.9.1",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@nestjs/cli": "9.0.0",
        "@nestjs/schematics": "9.0.1",
        "@nestjs/testing": "9.0.1",
        "@types/express": "4.17.13",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/node": "18.0.3",
        "@types/nodemailer": "^6.4.5",
        "@types/nodemailer-smtp-pool": "^1.0.32",
        "@types/supertest": "2.0.12",
        "@typescript-eslint/eslint-plugin": "5.30.5",
        "@typescript-eslint/parser": "5.30.5",
        "cross-env": "^7.0.3",
        "eslint": "8.19.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-import": "2.26.0",
        "jest": "28.1.2",
        "prettier": "2.7.1",
        "supertest": "6.2.4",
        "ts-jest": "28.0.5",
        "ts-loader": "9.3.1",
        "ts-morph": "15.1.0",
        "ts-node": "10.8.2",
        "tsconfig-paths": "4.0.0",
        "typescript": "4.7.4"
    }
}
